@media screen and (min-device-width : 320px) and (max-device-width : 600px)%
	
	.logo
		margin 45px auto 50px
		width 95%

	.goback
		font-size 250%
		padding 0 9px

	.flash-notification-message
		-webkit-transition opacity 3s
		transition opacity 3s
		opacity 1
		font-size 200%

	.flash-notification-message.ng-leave-active
		opacity 0

	.fromsigninmessage
		font-size 300%

	.toast-top-right 
		top 25%
		right 10%

	.toast-success 
		background-color #13987e

	.toast-warning
		background-color #222222
		color #dddddd
		font-size 400%
		width 300px

	#signin
		font-size 200%

	#signin input[type='email']
		margin-top 15px
		margin-bottom 13px

	#signin input[type='email'],
	#signin input[type='password']
		height 100px
		font-size 150%

	#signin input[type='password']
		margin-bottom 30px

	#signin button
		font-size 200%
		width 100%
		margin-bottom 25px

	#signin .forgot
		margin-bottom 40px
		font-size 150%

	#mainpage
		font-size 200%

	#mainpage .btn-group
		width 100%

	#mainpage .main-buttons 
		margin 100px 0 150px 0

	#mainpage .main-buttons a
		font-size 300%

	#mainpage .main-signin-button, 
	#mainpage .main-register-button
		background-color #F8B31E
		color #000000
		border 1px solid #000000

	#mainpage .main-signin-button:hover, 
	#mainpage .main-register-button:hover
		background-color #000000
		color #F8B31E
		border 1px solid #F8B31E

	#mainpage .main-message,
	#checkavailability .main-message
		margin 50px 0 60px 0
		font-size 500%

	#registerpage
		font-size 200%

	#registerpage h4
		font-size 300%

	#registerpage .registerform
		margin-top 100px

	#registerpage input[type='text'],
	#registerpage input[type='email'],
	#registerpage input[type='password'],
	#registerpage select
		height 90px
		font-size 300%

	#registerpage .cellphonevalidationmessage
		font-size 120%

	#checkavailability
		font-size 200%

	#checkavailability .checklabel
		display inline-block
		margin-right 30px
		font-size 250%

	#checkavailability .checkinput
		height 90px
		font-size 200%
		width 40%
		margin-bottom 70px

	#areanotserviced
		font-size 200%

	#areanotserviced a
		display inline-block
		margin-left 13px
		font-size 250%

	#areanotserviced .well
		width 100%

	#areanotserviced h5
		font-size 300%

	#areanotserviced p
		font-size 200%
	
	#areanotserviced #top
		margin-top 90px
		margin-bottom 50px

	#areanotserviced input[type='text'],
	#areanotserviced input[type='email']
		height 90px
		font-size 300%

	#areanotserviced .noservice-button
		font-size 200%
		background-color #F8B31E
		color #000000
		border 1px solid #000000
		width 100%

	#validationpage
		font-size 200%

	#validationpage a 
		font-size 250%

	#validationpage h4 
		font-size 300%

	#validationpage .validateform
		margin-top 80px

	#validationpage input[type='text']
		font-size 300%
		height 90px

	#validationpage .validation-message,
	#validationpage .validation-confirm
		margin 36px 0 0 0
		font-size 250%

	#validationpage .validate-btn[disabled],
	#validationpage .validate-btn[disabled]:hover
		background-color #cccccc

	#validationpage .validate-btn
		background-color #F8B31E
		border none
		color #000000
		width 100%
		font-size 200%
		margin-top 75px

	#validationpage .validate-btn:hover
		background-color #000000
		color #ffffff

	#validationpage .validationerror
		background-color #cc0000
		padding 5px 10px
		color #ffffff
		font-size 250%

	#verificationsuccesspage
		font-size 200%

	#verificationsuccesspage h4
		font-size 400%

	#verificationsuccesspage p
		font-size 200%

	#verificationsuccesspage input[type='text'],
	#verificationsuccesspage input[type='password']
		font-size 300%
		height 90px

	#verificationsuccesspage .small
		font-size 150%

	#verificationsuccesspage button
		background-color #F8B31E
		border none
		color #000000
		width 100%
		font-size 200%
		margin-top 75px

	#loginrecovery
		font-size 200%

	#loginrecovery h4
		font-size 300%
		padding-top 100px
		margin-bottom 70px

	#loginrecovery label
		font-size 200%

	#loginrecovery input[type='email'],
	#loginrecovery input[type='text']
		font-size 300%
		height 90px

	#loginrecovery .recoverbutton
		font-size 200%
		margin-top 250px
		padding 0 20px
		background-color #F8B31E
		color #000000
		border 1px solid #000000

	#accountrecovery
		font-size 200%

	#accountrecovery .recoverinfo
		font-size 250%
		padding 0 13px

	#accountrecovery input[type='email'],
	#accountrecovery input[type='text']
		font-size 300%
		height 90px

	#myaccount,
	#mainmenupage
		font-size 200%

	#myaccount h4
		font-size 300%

	#myaccount .mainmenu,
	#mainmenupage .mainmenu
		margin-top 100px

	#myaccount .btn-success
		margin-bottom 19px
		font-size 300%
		padding 35px 0
		background-color #F8B31E
		color #000000
		border 1px solid #000000

	#mainmenupage .menubutton
		margin-bottom 19px
		font-size 250%
		padding 10px 0
		background-color #F8B31E
		color #000000
		border 1px solid #000000

	#mainmenupage .cart-button
		font-size 150%

	#grocerybrowse
		font-size 200%
		margin-top 100px

	#grocerybrowse h4
		font-size 300%

	#grocerybrowse .cart-button
		font-size 150%

	#grocerybrowse .searchBox,
	#shoppingmethods .searchBox
		margin 0 0 20px 0
		font-size 240%
		height 70px

	#grocerybrowse .browse-image
		margin 0 auto

	#grocerybrowse .item_name,
	#grocerybrowse .item_price
		font-size 200%

	#grocerybrowse .listButton,
	#grocerybrowse .cartButton
		font-size 120%
		padding 10px 20px

	#shoppingmethods
		font-size 200%
		margin-top 100px

	#shoppingmethods h4
		font-size 300%

	#shoppingmethods .cart-button
		font-size 150%

	#shoppingmethods .btn-success
		margin-bottom 19px
		font-size 300%
		padding 35px 0
		background-color #F8B31E
		color #000000
		border 1px solid #000000

	#shopbyaisle,
	#shopmeatandseafood
		font-size 200%
		margin-top 100px

	#shopbyaisle h4
		font-size 250%

	#shopbyaisle .cart-button,
	#shopmeatandseafood .cart-button
		font-size 150%

	#shopbyaisle .btn-success
		margin-bottom 19px
		font-size 300%
		padding 20px 0
		background-color #F8B31E
		color #000000
		border 1px solid #000000

	#shopmeatandseafood h4
		font-size 200%

	#shopmeatandseafood table tr td
		font-size 200%

	#validatecreditcard
		font-size 200%

	#validatecreditcard h4
		font-size 300%

	#validatecreditcard p
		font-size 130%

	#validatecreditcard input[type='text'],
	#validatecreditcard select,
	#validatecreditcard label
		font-size 180%
		height 60px

	#validatecreditcard a.btn
		font-size 200%
		margin-top 30px
		padding 0 20px
		background-color #F8B31E
		color #000000
		border 1px solid #000000

	#validatecreditcard input[type='text']
		margin-bottom 10px

	#validatecreditcard ul li
		display inline-block
		margin 0 5px

	#validatecreditcard .cclabel 
		padding-bottom 0
		margin-bottom 0

	#validatecreditcard table
		width 100%
		padding 0
		background-color #dddddd

	#validatecreditcard table tr td
		padding 5px 0

	#validatecreditcard .ccform
		margin-bottom 700px

	#footer-menu
		font-size 200%

	#footer-menu .navbar-fixed-bottom
		padding-top 30px

	#validatecreditcard #creditcard-validate-menu
		font-size 200%
		margin-top 80px
		height 120px

	#validatecreditcard #creditcard-validate-menu ul li
		display inline-block
		margin 0 3px

	#validatecreditcard #creditcard-validate-menu a
		width 180px
		height 18px
		background-color #F8B31E
		border 1px solid #000000
		border-radius 0
		padding 0
		margin-top -50px

	#footer-menu ul,
	#validatecreditcard #creditcard-validate-menu ul
		list-style-type none
		padding 0
		margin 0

	#footer-menu .iconlabel
		font-size 200%

	#footer-menu ul li
		display inline-block
		margin 0 5px

	#footer-menu a
		width 180px
		height 180px
		-webkit-border-radius 40px
		-moz-border-radius 40px
		border-radius 40px
		background-color #F8B31E
		border 1px solid #000000